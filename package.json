    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "glob": "^10.3.10",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
